class Solution:
    def removeDuplicates(self, s: str, k: int) -> str:
        stack = deque([])
        out = ""
        for c in s:
            if stack and stack[-1][0] == c:
                stack[-1][1] += 1 
                if stack[-1][1] == k:
                    stack.pop()                           
            else:
                stack.append([c, 1])
        
        for i in stack:
            out += i[0]*i[1]
            
        return out
        
